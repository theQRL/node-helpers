{
  "name": "@theqrl/node-helpers",
  "version": "0.7.1",
  "description": "QRL node helpers",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel src --out-dir ./dist --source-maps",
    "lint": "eslint .",
    "test": "nyc ./node_modules/.bin/mocha --timeout 30000 --require @babel/register",
    "cover": "mkdir -p coverage && nyc --reporter=text ./node_modules/mocha/bin/_mocha test/*.js --timeout 10000 && nyc --reporter=text-lcov ./node_modules/mocha/bin/_mocha --timeout 10000 test/*.js > coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theqrl/node-helpers.git"
  },
  "author": "QRL contributors <info@theqrl.org> (https://theqrl.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theqrl/node-helpers/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "html"
    ],
    "require": [
      "@babel/register"
    ],
    "extension": [
      ".js"
    ],
    "cache": false,
    "temp-dir": "./dist/tests/.nyc-temp",
    "report-dir": "./dist/tests/nyc"
  },
  "homepage": "https://github.com/theqrl/node-helpers#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/node": "^7.18.5",
    "@babel/plugin-transform-async-to-generator": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@babel/register": "^7.17.7",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-modules": "^1.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^10.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-steps": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.13",
    "@theqrl/qrl-proto-sha256": "^3.5.0",
    "@theqrl/qrlbase.proto": "^1.1.1",
    "crypto-js": "^4.1.1",
    "google-proto-files": "^3.0.0",
    "google-protobuf": "^3.20.1",
    "tmp": "^0.2.1"
  }
}
